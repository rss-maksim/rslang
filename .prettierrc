{
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "overrides": [
        {
            "files": "*.component.html",
            "options": { "parser": "angular" }
        },
        {
            "files": "*.ts",
            "options": {
                "singleQuote": true,
                "trailingComma": "all"
            }
        }
    ]
}
